{
  "name": "nulla",
  "version": "0.9.2",
  "description": "CLI for Nullstack fed by Nulla-chan powers and will",
  "author": "GuiDevloper",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiDevloper/nulla"
  },
  "main": "./dist/index.js",
  "bin": {
    "nulla": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nullstack",
    "cli"
  ],
  "devDependencies": {
    "parcel": "2.5.0",
    "commander": "8.3.0"
  },
  "scripts": {
    "test": "jest --testTimeout=10000",
    "start": "parcel watch",
    "build": "parcel build",
    "prepublishOnly": "npm run build"
  },
  "targets": {
    "index": {
      "includeNodeModules": true,
      "optimize": true,
      "sourceMap": false,
      "source": "index.ts",
      "context": "node",
      "engines": {
        "node": ">=12.22.0"
      }
    }
  }
}
