{
  "name": "nulla",
  "version": "0.9.6",
  "description": "CLI for Nullstack fed by Nulla-chan powers and will",
  "author": "GuiDevloper",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiDevloper/nulla"
  },
  "main": "./dist/index.js",
  "bin": {
    "nulla": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nullstack",
    "cli"
  ],
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "chalk": "2.4.2",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prompts": "2.1.0",
    "rimraf": "3.0.0",
    "tar": "4.4.10",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0",
    "parcel": "2.5.0",
    "commander": "8.3.0"
  },
  "scripts": {
    "test": "jest --testTimeout=10000",
    "start": "parcel watch",
    "build": "rimraf ./dist/index.js && parcel build",
    "prepublishOnly": "npm run build"
  },
  "targets": {
    "index": {
      "includeNodeModules": true,
      "optimize": true,
      "sourceMap": false,
      "source": "index.ts",
      "context": "node",
      "engines": {
        "node": ">=12.22.0"
      }
    }
  }
}
